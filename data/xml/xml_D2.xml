<lb n="6" xml:id="D2f4r.6"/><l n="D2_M2_10_121"><hi rend="capitalsize1">O</hi>m iason dies haer cume<damage><gap></gap></damage></l>
<lb n="7" xml:id="D2f4r.7"/><l n="D2_M2_10_122"><hi rend="capitalsize1">L</hi>iet sij conincrijc <choice><abbr>en<g ref="#bar"/></abbr><expan>en<ex>de</ex></expan></choice> s<unclear>tadt</unclear></l>
<lb n="8" xml:id="D2f4r.8"/><l n="D2_M2_10_123"><hi rend="capitalsize1">V</hi>ader suster <choice><abbr>en<g ref="#bar"/></abbr><expan>en<ex>de</ex></expan></choice> m<damage><gap></gap></damage></l>
<lb n="9" xml:id="D2f4r.9"/><l n="D2_M2_10_124"><hi rend="capitalsize1">N</hi>ochtans daer sij<damage><gap></gap></damage>e<damage><gap></gap></damage></l>
<lb n="10" xml:id="D2f4r.10"/><l n="D2_M2_10_125"><hi rend="capitalsize1">W</hi>ijsde haer red<unclear>e</unclear><damage><gap></gap></damage></l>


<pb xml:id="D2.f4v" n="4v"/>
</lg>
