<lb n="8" xml:id="D2f4v.8"/><l n="D2_M2_12_144"><damage><gap></gap></damage>gh <damage><gap></gap></damage>n stout sergiant</l>
<lb n="9" xml:id="D2f4v.9"/><l n="D2_M2_12_145"><damage><gap></gap></damage>n <damage><gap></gap></damage> op <choice><abbr>en<g ref="#bar"/></abbr><expan>en<ex>de</ex></expan></choice> vast bant</l>
<lb n="10" xml:id="D2f4v.10"/><l n="D2_M2_12_146"><damage><gap></gap></damage>n ontbinden</l>


